{
	"$schema": "https://biomejs.dev/schemas/1.7.3/schema.json",
	"organizeImports": {
		"enabled": false
	},
	"json": {
		"formatter": {
			"enabled": true
		}
	},
	"javascript": {
		"formatter": {
			"semicolons": "asNeeded",
			"quoteStyle": "single",
			"bracketSpacing": true,
			"indentStyle": "space",
			"lineWidth": 90,
			"indentWidth": 2,
			"lineEnding": "lf",
			"arrowParentheses": "always"
		}
	}
}
